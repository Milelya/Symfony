
RewriteEngine on


	## http -> https
RewriteCond %{HTTPS} off
RewriteRule ^ https://%{HTTP_HOST}%{REQUEST_URI} [R=301,L,NE,QSA]



RewriteCond %{HTTP_HOST} ^opteamom.com$ 
RewriteCond %{HTTP_HOST} ^www.opteamom\.com$ 
RewriteCond %{SERVER_PORT} 80 
Header always set Strict-Transport-Security "max-age=31536000" env=HTTPS
RewriteRule ^(.*) https://www.opteamom.com/$1 [QSA,L,R]



RewriteCond %{REQUEST_URI} !index.php
RewriteCond %{REQUEST_FILENAME} !style.css
RewriteCond %{REQUEST_FILENAME} !activity.css
RewriteCond %{REQUEST_FILENAME} !activity.js
RewriteCond %{REQUEST_FILENAME} !resume.js
RewriteCond %{REQUEST_FILENAME} !app.js
RewriteCond %{REQUEST_FILENAME} !opteamomPreview.png
RewriteCond %{REQUEST_FILENAME} !favicon.png
RewriteRule (.*) index.php?var=$1
